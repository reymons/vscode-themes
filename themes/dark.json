{
  "name": "Rmns Dark Theme",
  "colors": {
    "editor.background": "#161616",
    "editor.foreground": "#c9c9c9",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#161616",
    "sideBar.border": "#232323"
  },
  "tokenColors": [
    {
      "name": "Default",
      "scope": ["storage.type.function.arrow", "variable.other.constant"],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "name": "Bracket, dot, pointer",
      "scope": [
        "meta.brace",
        "punctuation.accessor",
        "punctuation.separator.dot-access",
        "punctuation.section.arguments",
        "punctuation.section.parens",
        "punctuation.section.block.begin.bracket",
        "punctuation.section.block.end.bracket"
      ],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "name": "Function declaration bracket",
      "scope": [
        "punctuation.section.parameters.begin.bracket",
        "punctuation.section.parameters.end.bracket",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end"
      ],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "storage.type",
        "keyword.control",
        "keyword.other",
        "punctuation.definition.directive",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#7bb3de"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string",
        "punctuation.definition.comment",
        "comment.block",
        "variable.parameter.url"
      ],
      "settings": {
        "foreground": "#7cb772"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "keyword.other.unit.suffix"],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "name": "Function",
      "scope": ["meta.function-call", "meta.function"],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "name": "JSX Component",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#dee57d"
      }
    },
    {
      "name": "JSX Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ed6c6c"
      }
    },
    {
      "name": "CSS property value",
      "scope": ["meta.property-value.css", "meta.property-value.scss"],
      "settings": {
        "foreground": "#7bb3de"
      }
    },
    {
      "name": "CSS numeric",
      "scope": ["constant.numeric.css", "constant.numeric.scss"],
      "settings": {
        "foreground": "#7bb3de"
      }
    }
  ]
}
